{
  "metadata": [
    {
      "src": [
        {
          "src": "../../",
          "files": [
            "XRENGINE.sln"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "**/Submodules/**"
          ]
        }
      ],
      "dest": "apiref",
      "filter": "filterConfig.yml",
      "allowCompilationErrors": true,
      "properties": {
        "RunAnalyzersDuringBuild": "true",
        "EnableNETAnalyzers": "true",
        "NoWarn": "NU1510"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "index.md",
            "guides.md",
            "toc.yml",
            "api/index.md"
        ]
      },
      {
        "src": "..",
        "files": [
          "DEPENDENCIES.md",
            "README.md",
          "api/**/*.md",
          "api/toc.yml",
          "architecture/**/*.md",
          "architecture/toc.yml",
            "features/**/*.md",
            "features/toc.yml",
            "work/**/*.md"
        ]
      },
      {
        "files": [
          "apiref/**.yml",
          "apiref/index.md",
          "apiref/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "styles/**"
        ]
        },
        {
          "src": "..",
          "files": [
            "licenses/**"
          ]
        },
        {
          "src": "../..",
          "files": [
            "LICENSE"
          ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "XRENGINE",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/BlackJaxDev/XRENGINE",
        "branch": "master",
        "path": "/"
      }
    },
    "template": [
      "default"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "styles": [
      "styles/custom.css"
    ],
    "scripts": [
      "styles/custom.js"
    ]
  }
}
