{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "build",
                "${workspaceFolder}/XREngine.Editor/XREngine.Editor.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "group": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build-server",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "build",
                "${workspaceFolder}/XREngine.Server/XREngine.Server.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "group": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build-vrclient",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "build",
                "${workspaceFolder}/XREngine.VRClient/XREngine.VRClient.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "group": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "start-editor-no-debug",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "set XRE_WINDOW_TITLE=XRE Editor (Separate) && start \"\" /D \"${workspaceFolder}\\XREngine.Editor\" \"${workspaceFolder}\\Build\\Editor\\Debug\\AnyCPU\\Debug\\net10.0-windows7.0\\XREngine.Editor.exe\""
            ],
            "dependsOn": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "start-server-no-debug",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "set XRE_NET_MODE=Server && start \"\" /D \"${workspaceFolder}\\XREngine.Editor\" \"${workspaceFolder}\\Build\\Editor\\Debug\\AnyCPU\\Debug\\net10.0-windows7.0\\XREngine.Editor.exe\""
            ],
            "dependsOn": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "start-client-no-debug",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "set XRE_NET_MODE=Client && set XRE_UDP_CLIENT_RECEIVE_PORT=5001 && set XRE_WINDOW_TITLE=XRE Editor (Client 1) && start \"\" /D \"${workspaceFolder}\\XREngine.Editor\" \"${workspaceFolder}\\Build\\Editor\\Debug\\AnyCPU\\Debug\\net10.0-windows7.0\\XREngine.Editor.exe\""
            ],
            "dependsOn": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "start-client2-no-debug",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "set XRE_NET_MODE=Client && set XRE_UDP_CLIENT_RECEIVE_PORT=5002 && set XRE_WINDOW_TITLE=XRE Editor (Client 2) && start \"\" /D \"${workspaceFolder}\\XREngine.Editor\" \"${workspaceFolder}\\Build\\Editor\\Debug\\AnyCPU\\Debug\\net10.0-windows7.0\\XREngine.Editor.exe\""
            ],
            "dependsOn": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "start-2-clients-no-debug",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "set XRE_NET_MODE=Client && set XRE_UDP_CLIENT_RECEIVE_PORT=5001 && set XRE_WINDOW_TITLE=XRE Editor (Client 1) && start \"\" /D \"${workspaceFolder}\\XREngine.Editor\" \"${workspaceFolder}\\Build\\Editor\\Debug\\AnyCPU\\Debug\\net10.0-windows7.0\\XREngine.Editor.exe\" && timeout /t 1 /nobreak > nul && set XRE_UDP_CLIENT_RECEIVE_PORT=5002 && set XRE_WINDOW_TITLE=XRE Editor (Client 2) && start \"\" /D \"${workspaceFolder}\\XREngine.Editor\" \"${workspaceFolder}\\Build\\Editor\\Debug\\AnyCPU\\Debug\\net10.0-windows7.0\\XREngine.Editor.exe\""
            ],
            "dependsOn": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "start-dedicated-server-no-debug",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "start \"\" /D \"${workspaceFolder}\\XREngine.Server\" \"${workspaceFolder}\\XREngine.Server\\bin\\Debug\\net10.0-windows10.0.26100.0\\XREngine.Server.exe\""
            ],
            "dependsOn": "build-server",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "start-p2p-peer-no-debug",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "set XRE_NET_MODE=P2PClient && set XRE_UDP_CLIENT_RECEIVE_PORT=5001 && set XRE_WINDOW_TITLE=XRE Editor (P2P Peer 1) && start \"\" /D \"${workspaceFolder}\\XREngine.Editor\" \"${workspaceFolder}\\Build\\Editor\\Debug\\AnyCPU\\Debug\\net10.0-windows7.0\\XREngine.Editor.exe\""
            ],
            "dependsOn": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "start-p2p-peer2-no-debug",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "set XRE_NET_MODE=P2PClient && set XRE_UDP_CLIENT_RECEIVE_PORT=5002 && set XRE_WINDOW_TITLE=XRE Editor (P2P Peer 2) && start \"\" /D \"${workspaceFolder}\\XREngine.Editor\" \"${workspaceFolder}\\Build\\Editor\\Debug\\AnyCPU\\Debug\\net10.0-windows7.0\\XREngine.Editor.exe\""
            ],
            "dependsOn": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "prep-debug-server-with-2-clients",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "echo Preparing dedicated server debug session..."
            ],
            "dependsOn": [
                "build-server",
                "start-2-clients-no-debug"
            ],
            "dependsOrder": "sequence",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "prep-debug-server-only",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "echo Preparing dedicated server debug session (server only)..."
            ],
            "dependsOn": [
                "build-server"
            ],
            "dependsOrder": "sequence",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "prep-debug-client-with-server-and-client",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "echo Preparing client debug session (server + extra client)..."
            ],
            "dependsOn": [
                "start-dedicated-server-no-debug",
                "start-client2-no-debug"
            ],
            "dependsOrder": "sequence",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "prep-debug-p2p-client-with-peer",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "echo Preparing P2P client debug session (peer client only)..."
            ],
            "dependsOn": [
                "start-p2p-peer2-no-debug"
            ],
            "dependsOrder": "sequence",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "prep-debug-vrclient-with-editor",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "echo Preparing VRClient debug session (editor runs separately)..."
            ],
            "dependsOn": [
                "start-editor-no-debug",
                "build-vrclient"
            ],
            "dependsOrder": "sequence",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "test-surfelgi",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "test",
                "${workspaceFolder}/XREngine.UnitTests/XREngine.UnitTests.csproj",
                "-c",
                "Debug",
                "--filter",
                "FullyQualifiedName~XREngine.UnitTests.Rendering.SurfelGiComputeIntegrationTests"
            ],
            "group": "test",
            "presentation": {
                "reveal": "always"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build-profiler",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "build",
                "${workspaceFolder}/XREngine.Profiler/XREngine.Profiler.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "group": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "start-profiler-no-debug",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "start \"\" /D \"${workspaceFolder}\\XREngine.Profiler\" \"${workspaceFolder}\\XREngine.Profiler\\bin\\Debug\\net10.0-windows7.0\\XREngine.Profiler.exe\""
            ],
            "dependsOn": "build-profiler",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        },
        {
            "label": "start-editor-with-profiler-no-debug",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/d",
                "/c",
                "set XRE_PROFILER_ENABLED=1 && set XRE_WINDOW_TITLE=XRE Editor (Profiled) && start \"\" /D \"${workspaceFolder}\\XREngine.Editor\" \"${workspaceFolder}\\Build\\Editor\\Debug\\AnyCPU\\Debug\\net10.0-windows7.0\\XREngine.Editor.exe\""
            ],
            "dependsOn": "build",
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": []
        }
    ]
}